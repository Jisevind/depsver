{
  "name": "depsver",
  "version": "1.0.0",
  "description": "A TypeScript CLI tool for analyzing project dependencies and generating AI-ready reports",
  "main": "dist/cli.js",
  "bin": {
    "depsver": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm src/cli.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
