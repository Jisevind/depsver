  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "clipboardy": "^3.0.0",
    "cli-progress": "^3.12.0",
    "semver": "^7.5.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.5.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.9.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
